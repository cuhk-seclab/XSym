    ai_p="$MYSQL_LIBDIR"
  else
    ai_p="`pwd`/$MYSQL_LIBDIR"
  fi

    
  unique=`echo $ac_n "$ai_p$ac_c" | tr -c -d a-zA-Z0-9`
  cmd="echo $ac_n \"\$LIBPATH$unique$ac_c\""
  if test -n "$unique" && test "`eval $cmd`" = "" ; then
